<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>SeleniumExtention</id>
    <version>1.0.0</version>
    <title>Selenium Network Monitor with Accessibility Extensions and Excel Export</title>
    <authors>Open Source</authors>
    <owners>Open Source</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A comprehensive Selenium WebDriver addon for monitoring network traffic (requests and responses),
      exporting the full network information to Excel using NPOI, and providing helpful
      WebDriver extension methods for locating elements by ARIA roles.
    </description>
    <releaseNotes>
      Initial release: Network traffic capture, Excel export (NPOI), and WebDriver extensions for ARIA role-based element location.
    </releaseNotes>
    <copyright>Copyright 2025</copyright>
    <tags>Selenium WebDriver Network DevTools Traffic Monitoring Excel NPOI ARIA Accessibility Locators Extensions</tags>
    <dependencies>
      <dependency id="Selenium.WebDriver" version="4.34.0" /> 
      <dependency id="Selenium.Support" version="4.34.0" />
      <dependency id="DotNetSeleniumExtras.WaitHelpers" version="3.11.0" /> 
      <dependency id="NPOI" version="2.7.4" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\SeleniumNetworkMonitor.dll" target="lib\net8.0" /> 
    <file src="bin\$configuration$\SeleniumNetworkMonitor.pdb" target="lib\net8.0" />
  </files>
</package>