<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>SimpleSeleniumSupport</id>
    <license type="file">LICENSE</license>
    <readme>README.md</readme>
    <version>1.3.0</version>
    <title>AI-Powered Selenium WebDriver Toolkit</title>
    <authors>Anand</authors>
    <owners>Anand</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A comprehensive Selenium WebDriver addon for advanced, AI-driven automation. Features include:
      - **AI Element Finding:** Use natural language (e.g., "the login button") to find elements with `FindElementByAI`, making tests highly resilient to UI changes.
      - **AI Failure Analysis:** Automatically analyze exceptions and suggest code fixes.
      - **Advanced Network Control:** Monitor traffic and synchronize tests by waiting for network requests.
      - **Rich Selectors:** Use Playwright-style and ARIA role-based element locators.
    </description>
    <copyright>Copyright 2025</copyright>
    <tags>Selenium AI WebDriver Playwright DevTools Network Monitor Wait Sync ARIA Test Analysis Fix Llama Automation</tags>
    <dependencies>
      <dependency id="Selenium.WebDriver" version="4.34.0" />
      <dependency id="Selenium.Support" version="4.34.0" />
      <dependency id="DotNetSeleniumExtras.WaitHelpers" version="3.11.0" />
      <dependency id="NPOI" version="2.7.4" />
      <dependency id="System.Text.Json" version="8.0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\SimpleSeleniumSupport.dll" target="lib\net8.0" />
    <file src="bin\$configuration$\SimpleSeleniumSupport.pdb" target="lib\net8.0" />
    <file src="LICENSE" target="" />
    <file src="README.md" target="" />
  </files>
</package>